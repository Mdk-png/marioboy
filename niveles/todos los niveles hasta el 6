            objetos.add(new Objeto(900, ypiso - 10, 30, 2, Color.black));
            
            objetos.add(new ObjetoMeta(this, 900, ypiso - 10, 30, 50, Color.MAGENTA)); 
            
            
            
        }
        else if (nivel == 1) {        

            objetos.add(new ObjetoMovil(300, ypiso - 130,  80, 10, Color.black, 3, 0, 900)); //plataforma grande
            objetos.add(new ObjetoMovil(500, ypiso - 230,  80, 10, Color.black, 3, 0, 900)); //plataforma grande
            objetos.add(new ObjetoMovil(0, ypiso - 430,  80, 10, Color.black, 3, 0, 900)); //plataforma grande
            objetos.add(new ObjetoMovil(900, ypiso - 330,  80, 10, Color.black, 3, 0, 900)); //plataforma grande
         
            objetos.add(new Objeto(850, ypiso - 50, 100, 140, Color.black));
            

            objetos.add(new ObjetoMalo(220, ypiso + 0 , 127, 40, Color.RED));
            objetos.add(new ObjetoMalo(420, ypiso + 0 , 127, 40, Color.RED));

            objetos.add(new ObjetoMalo(620, ypiso + 0 , 127, 40, Color.RED));
        	objetos.add(new Objeto(-1, ypiso - 900, 1, 1000, Color.black)); //paredes
        	objetos.add(new Objeto(999, ypiso - 900, 5, 1000, Color.black)); 
            objetos.add(new Objeto(900, ypiso - 500, 30, 2, Color.black)); 
      objetos.add(new ObjetoMeta(this,900, ypiso - 500, 30, 50, Color.MAGENTA)); 
        }
        
    }
