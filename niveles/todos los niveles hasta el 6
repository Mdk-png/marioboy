
    void inicializarObjetos() {
        objetos.clear(); // Limpiar la lista de objetos
        // Añadir objetos dependiendo del nivel
        if (nivel == 10) {
        	objetos.add(new Objeto(-1, ypiso - 900, 1, 1000, Color.black)); //paredes
        	objetos.add(new Objeto(999, ypiso - 900, 5, 1000, Color.black)); 
            
        	
        	objetos.add(new Objeto(300, ypiso - 50, 100, 40, Color.black)); 
            objetos.add(new Objeto(450, ypiso - 100, 50, 40, Color.black));
            objetos.add(new Objeto(550, ypiso - 50, 100, 40, Color.black));
            objetos.add(new Objeto(865, ypiso - 0, 150, 40, Color.black));
            objetos.add(new Objeto(900, ypiso - 50, 30, 2, Color.black)); 
      objetos.add(new ObjetoMeta(this,900, ypiso - 50, 30, 50, Color.MAGENTA)); 
        
        } 
        else if (nivel == 2) {
         	objetos.add(new Objeto(-1, ypiso - 900, 1, 1000, Color.black)); //paredes
        	objetos.add(new Objeto(999, ypiso - 900, 5, 1000, Color.black)); 
            
            objetos.add(new Objeto(170, ypiso - 50, 100, 40, Color.black)); 
            objetos.add(new Objeto(320, ypiso - 100, 50, 40, Color.black));
            objetos.add(new Objeto(420, ypiso - 150, 100, 40, Color.black)); 
            objetos.add(new Objeto(600, ypiso - 200, 100, 300, Color.black)); 

            objetos.add(new Objeto(900, ypiso - 10, 30, 2, Color.black)); 
        objetos.add(new ObjetoMeta(this,900, ypiso - 10, 30, 50, Color.MAGENTA)); 
        
        
        
        }
        else if (nivel == 3) {
         	objetos.add(new Objeto(-1, ypiso - 900, 1, 1000, Color.black)); //paredes
        	objetos.add(new Objeto(999, ypiso - 900, 5, 1000, Color.black)); 
            
        	   objetos.add(new Objeto(100, ypiso - 50, 100, 200, Color.black)); 
               objetos.add(new Objeto(250, ypiso - 100, 50, 40, Color.black)); 
               objetos.add(new Objeto(500, ypiso - 100, 100, 200, Color.black)); 
               objetos.add(new Objeto(700, ypiso - 50, 100, 200, Color.black)); 

               objetos.add(new Objeto(900, ypiso - 10, 30, 2, Color.black)); 
         objetos.add(new ObjetoMeta(this, 900, ypiso - 10, 30, 50, Color.MAGENTA)); 
               
        
        
        }
        else if (nivel == 4) {
         	objetos.add(new Objeto(-1, ypiso - 900, 1, 1000, Color.black)); //paredes
        	objetos.add(new Objeto(999, ypiso - 900, 5, 1000, Color.black)); 
            
            objetos.add(new Objeto(150, ypiso - 200, 100, 350, Color.black)); //plataforma grande
            objetos.add(new Objeto(650, ypiso - 200, 100, 350, Color.black)); //plataforma grande 2
            objetos.add(new Objeto(100, ypiso - 50, 100, 40, Color.black)); //primer escalon
            objetos.add(new Objeto(0, ypiso - 100, 50, 40, Color.black)); //segundo 
            objetos.add(new Objeto(100, ypiso - 170, 100, 40, Color.black)); //tercer escalon

            objetos.add(new Objeto(250, ypiso - 50, 50, 40, Color.black)); //4escalon
            objetos.add(new Objeto(250, ypiso - 150, 20, 40, Color.black)); //4escalon
            objetos.add(new ObjetoMovil(700, ypiso - 200, 100, 40, Color.BLUE, 2, 250, 600 )); //objeto móvil

            objetos.add(new Objeto(900, ypiso - 10, 30, 2, Color.black));
            objetos.add(new ObjetoMeta(this, 900, ypiso - 10, 30, 50, Color.MAGENTA)); 
            
            
            
        }
        else if (nivel == 5) {
        	objetos.add(new Objeto(-1, ypiso - 900, 1, 1000, Color.black)); //paredes
        	objetos.add(new Objeto(999, ypiso - 900, 5, 1000, Color.black)); 
            
            objetos.add(new Objeto(150, ypiso - 00, 100, 50, Color.black)); //plataforma grande
            objetos.add(new Objeto(750, ypiso - 170, 100, 500, Color.black)); //plataforma grande 2

            objetos.add(new ObjetoMalo(250, ypiso + 0 , 22, 40, Color.RED));
